      {
          "name": "sqlbuildmanager{{counter}}",
          "properties": {
              "image": "{{registryServer}}/{{imagename}}:{{tag}}",
              "command": [
                  "dotnet",
                  "sbm.dll",
                  "aci",
                  "worker"
              ],
            "environmentVariables": [
              {
                "name": "Sbm_KeyVaultName",
                "value": "{{keyVaultName}}"
              },
              {
                "name": "Sbm_JobName",
                "value": "{{jobname}}"
              },
              {
                "name": "Sbm_PackageName",
                "value": "{{packageName}}"
              },
              {
                "name": "Sbm_DacpacName",
                "value": "{{dacpacName}}"
              },
              {
                "name": "Sbm_Concurrency",
                "value": "{{concurrency}}"
              },
              {
                "name": "Sbm_ConcurrencyType",
                "value": "{{concurrencyType}}"
              },

              {
                "name": "Sbm_IdentityClientId",
                "value": "{{identityClientId}}"
              },
              {
                "name": "Sbm_AllowObjectDelete",
                "value": "{{allowObjectDelete}}"
              },
              {
                "name": "Sbm_AuthType",
                "value": "{{authType}}"
              },
              {
                "name": "Sbm_EventHubConnectionString",
                "value": "{{eventHub}}"
              },
              {
                "name": "Sbm_ServiceBusTopicConnectionString",
                "value": "{{serviceBus}}"
              }



            ],
              "resources": {
                  "requests": {
                      "cpu": 1,
                      "memoryInGB": 1
                  }
              }
          }
      }