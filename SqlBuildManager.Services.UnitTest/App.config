<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>
  <appSettings>
    <add key="StorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=sqlbuildstorage;AccountKey=CCVL3xzOyVdK1vGg5OsA/lQByBJGPvfzit8yOkz4L3hHI/mtEu+3LdRvjYopVe3qxeTGTi2pRvbvAjfoNE6tjA=="/>
      <add key="DynamicHttpEndpointTemplate" value="http://[[ServerName]]:8675/BuildService.svc"/>
      <add key="DynamicTcpEndpointTemplate" value="net.tcp://[[ServerName]]:8676/SqlBuildManager.Services/BuildService"/>
  </appSettings>



  <log4net debug="true">
    <appender name="StandardRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="SqlBuildManager.AzureStorage.UnitTest.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="2000KB"/>
      <staticLogFileName value="true"/>
      <immediateFlush value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [Thread:%2thread] %-5level (%logger) - %message%newline"/>
      </layout>
    </appender>
    <root>
      <priority value="DEBUG"/>
      <appender-ref ref="StandardRollingLogFileAppender"/>

    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.3"/>
  </startup>
</configuration>
